# GitHub Integration Configuration (Optional)
# If provided, enables GitHub-based team collaboration features
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GITHUB_OWNER=your-organization-name
GITHUB_REPO=your-repository-name

# Discord Bot Integration (Optional)
# Enables Discord slash commands for team collaboration
DISCORD_TOKEN=your_discord_bot_token

# Telegram Bot Integration (Optional) 
# Enables Telegram bot commands for team notifications
TELEGRAM_TOKEN=your_telegram_bot_token

# Debug Mode (Optional)
# Enables detailed logging for troubleshooting
CLAUDE_SPEC_DEBUG=true
GITHUB_DEBUG=true

# Dashboard Configuration (Optional)
# Default port and tunnel settings
DASHBOARD_PORT=8000
TUNNEL_PROVIDER=ngrok